{
  "name": "aurora",
  "version": "1.0.0",
  "description": "Admin panel for Aurora",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production && npm run copy-tinymce",
    "watch": "webpack --watch",
    "copy-tinymce": "shx rm -rf ../../public/assets/js/tinymce && shx cp -r node_modules/tinymce ../../public/assets/js/tinymce",
    "postinstall": "npm run copy-tinymce"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "@tinymce/tinymce-react": "^6.3.0",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.22.3",
    "tinymce": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.6.0",
    "shx": "^0.4.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "author": "usbac",
  "license": "MIT"
}
